{
  "display_information": {
    "name": "Agent-Ops",
    "description": "AI coding agent â€” send prompts, get results",
    "background_color": "#1a1a2e"
  },
  "features": {
    "bot_user": {
      "display_name": "Agent-Ops",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "app_mentions:read",
        "channels:history",
        "chat:write",
        "chat:write.public",
        "files:read",
        "groups:history",
        "im:history",
        "im:read",
        "im:write",
        "users:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "request_url": "https://YOUR_WORKER_URL/channels/slack/events",
      "bot_events": [
        "app_mention",
        "message.channels",
        "message.groups",
        "message.im"
      ]
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}
